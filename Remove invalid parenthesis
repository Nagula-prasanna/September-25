class Solution(object):
    def removeInvalidParentheses(self, s):
        def is_valid(string):
            count = 0
            for char in string:
                if char == '(':
                    count += 1
                elif char == ')':
                    if count == 0:
                        return False
                    count -= 1
            return count == 0

        level = {s}
        while True:
            valid = list(filter(is_valid, level))
            if valid:
                return valid
            next_level = set()
            for item in level:
                for i in range(len(item)):
                    if item[i] not in ('(', ')'):
                        continue
                    next_state = item[:i] + item[i+1:]
                    next_level.add(next_state)
            level = next_level
