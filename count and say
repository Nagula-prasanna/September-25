class Solution(object):
    def countAndSay(self, n):
        result = "1"
        for _ in range(n - 1):
            prev = result
            result = ""
            i = 0
            while i < len(prev):
                count = 1
                while i + 1 < len(prev) and prev[i] == prev[i + 1]:
                    i += 1
                    count += 1
                result += str(count) + prev[i]
                i += 1
        return result
