from math import ceil
class Solution(object):
    def minEatingSpeed(self, piles, h):
        low = 1
        high = max(piles)
        while low <= high:
            mid = (low + high) // 2
            time = 0
            for pile in piles:
                time += int(ceil(float(pile) / mid))
            if time <= h:
                high = mid - 1
            else:
                low = mid + 1
        return low
